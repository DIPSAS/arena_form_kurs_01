                <term_definitions code="at0021">
                    <items id="text">≤8 eller ≥25</items>
                    <items id="description">Respirasjonsfrekvensen målt, mindre enn eller lik 8, eller større enn eller lik 25 pust per min; skåres som 3 poeng.</items>
                </term_definitions>
                <term_definitions code="at0022">
                    <items id="text">36.1-38.0</items>
                    <items id="description">Kroppstemperatur målt fra og med 36,1 til og med 38,0 grader Celsius; skåres til 0 poeng.</items>
                </term_definitions>
                <term_definitions code="at0023">
                    <items id="text">35.1-36.0 eller 38.1-39.0</items>
                    <items id="description">Kroppstemperatur målt fra og med 35,1 til og med 36,0 grader Celsius eller fra og med 38,1 til og med 39,0 grader Celsius; skåres til 1 poeng.</items>
                </term_definitions>
                <term_definitions code="at0024">
                    <items id="text">A</items>
                    <items id="description">Pasienten er helt våken; skåres som 0 poeng.</items>
                </term_definitions>
                <term_definitions code="at0025">
                    <items id="text">V, P eller U</items>
                    <items id="description">V - Reagerer på lyd/ tiltale, P - Reagerer på smerte, U - Reagerer ikke; skåres som 3 poeng.</items>
                </term_definitions>
                <term_definitions code="at0028">
                    <items id="text">Totalskår</items>
                    <items id="description">Summen av skår til hvert enkelt NEWS-parameter.</items>
                    <items id="comment">Totalskår vil normalt beregnes automatisk i en applikasjon.</items>
                </term_definitions>
                <term_definitions code="at0029">
                    <items id="text">Oksygenmetning</items>
                    <items id="description">Pasientens oksygenmetning.</items>
                </term_definitions>
                <term_definitions code="at0030">
                    <items id="text">≥96</items>
                    <items id="description">Oksygenmetning målt større enn eller lik 96 prosent; skåres som 0 poeng.</items>
                </term_definitions>
                <term_definitions code="at0031">
                    <items id="text">94-95</items>
                    <items id="description">Oksygenmetning målt fra og med 94 til og med 95 prosent; skåres som 1 poeng.</items>
                </term_definitions>
                <term_definitions code="at0032">
                    <items id="text">92-93</items>
                    <items id="description">Oksygenmetning målt fra og med 92 til og med 93 prosent; skåres som 2 poeng.</items>
                </term_definitions>
                <term_definitions code="at0033">
                    <items id="text">≤91</items>
                    <items id="description">Oksygenmetning målt som mindre enn eller lik 91 prosent; skåres som 3 poeng.</items>
                </term_definitions>
                <term_definitions code="at0034">
                    <items id="text">Tilførsel av oksygen</items>
                    <items id="description">Om pasienten tilføres oksygen for å opprettholde oksygenmetningen.</items>
                </term_definitions>
                <term_definitions code="at0036">
                    <items id="text">Nei</items>
                    <items id="description">Pasienten tilføres ikke oksygen; skåres som 0 poeng.</items>
                </term_definitions>
                <term_definitions code="at0037">
                    <items id="text">Ja</items>
                    <items id="description">Pasienten tilføres oksygen; skåres som 2 poeng.</items>
                </term_definitions>
                <term_definitions code="at0038">
                    <items id="text">≥39,1</items>
                    <items id="description">Kroppstemperatur målt større enn eller lik 39,1 grader Celcius; skåres som 2 poeng.</items>
                </term_definitions>
                <term_definitions code="at0039">
                    <items id="text">≤35</items>
                    <items id="description">Kroppstemperatur målt mindre enn eller lik 35,0 grader Celcius; skåres som 3 poeng.</items>
                </term_definitions>
                <term_definitions code="at0041">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0043">
                    <items id="text">Konfunderende faktorer</items>
                    <items id="description">Kommentar til og registrering av faktorer som kan ha betydning for skåren.</items>
                    <items id="comment">For eksempel om pasienten har KOLS.</items>
                </term_definitions>
                <term_definitions code="at0044">
                    <items id="text">Kommentar</items>
                    <items id="description">Ytterligere kommentar til NEWS-skåringen som ikke er fanget opp i andre felt.</items>
                </term_definitions>
                <term_definitions code="at0045">
                    <items id="text">Tree</items>
                    <items id="description">@ internal @</items>
                </term_definitions>
                <term_definitions code="at0046">
                    <items id="text">Tilleggsinformasjon</items>
                    <items id="description">Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.</items>
                    <items id="comment">For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR eller CIMI.</items>
                </term_definitions>
                <term_definitions code="at0000">
                    <items id="text">NEWS</items>
                    <items id="description">NEWS (National Early Warning Score) er et skåringsverktøy som benyttes for å gi en objektiv vurdering av grad av klinisk forverring hos en pasient. Denne versjonen følger retningslinjer utgitt av the UK Royal College of Physicians.</items>
                </term_definitions>
            </children>
